<p id="notice"><%= notice %></p>

<h1>Listing Precencia</h1>

<table>
  <thead>
    <tr>
      <th>Hora</th>
      <th>Observacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @precencia.each do |precencium| %>
      <tr>
        <td><%= precencium.hora %></td>
        <td><%= precencium.observacion %></td>
        <td><%= link_to 'Show', precencium %></td>
        <td><%= link_to 'Edit', edit_precencium_path(precencium) %></td>
        <td><%= link_to 'Destroy', precencium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Precencium', new_precencium_path %>
